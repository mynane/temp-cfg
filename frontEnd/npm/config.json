{
  "meta": [
    {
      "handle": "Select",
      "options": ["ts", "es"],
      "placeholder": "请选择js语言类型",
      "key": "jsType",
      "isRequired": true
    },
    {
      "handle": "Select",
      "options": ["less", "sass", "css"],
      "placeholder": "请选择样式类型",
      "key": "styleType",
      "isRequired": true
    },
    {
      "handle": "Input",
      "placeholder": "项目名称",
      "key": "name",
      "isRequired": true
    },
    {
      "handle": "Confirm",
      "placeholder": "是否确认以上选择",
      "key": "isOk",
      "isRequired": true
    }
  ],
  "files": [
    {
      "name": "{{ name }}/package.json",
      "content": "{\"name\": \"{{name}}\",\"private\": true,\"version\": \"0.0.1\"}"
    },
    {
      "name": "{{ name }}/src/package.{% if jsType == 'ts' %}ts{% else %}js{% endif %}",
      "template": "frontEnd/npm/templates/jsx.temp"
    }
  ]
}
