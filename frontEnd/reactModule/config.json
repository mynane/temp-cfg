{
  "meta": [
    {
      "handle": "Select",
      "options": ["tsx", "jsx"],
      "placeholder": "请选择js语言类型",
      "key": "jsType"
    },
    {
      "handle": "Select",
      "options": ["less", "scss", "css"],
      "placeholder": "请选择样式类型",
      "key": "styleType"
    },
    {
      "handle": "Confirm",
      "placeholder": "是否使用css module",
      "key": "useCssModule"
    },
    {
      "handle": "Input",
      "placeholder": "项目名称",
      "key": "name"
    },
    {
      "handle": "Confirm",
      "placeholder": "是否确认以上选择",
      "key": "isOk",
      "false_close": true
    }
  ],
  "files": [
    {
      "name": "{{ name }}/index.{{ jsType }}",
      "template": "frontEnd/reactModule/templates/jsx.temp"
    },
    {
      "name": "{{ name }}/index.{{ styleType }}",
      "template": "frontEnd/reactModule/templates/style.temp"
    }
  ]
}
